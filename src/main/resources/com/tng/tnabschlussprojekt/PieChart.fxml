<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tng.tnabschlussprojekt.gui.PieChartController">
   <children>
      <VBox>
         <children>
            <Label alignment="CENTER" prefHeight="27.0" prefWidth="800.0" text="Stock">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Label>
            <PieChart fx:id="pieChart" prefHeight="685.0" prefWidth="720.0" />
            <HBox>
               <children>
                  <Button alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#toDashboardBtn" text="Back">
                     <HBox.margin>
                        <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#toBarChart" text="Next">
                     <HBox.margin>
                        <Insets bottom="40.0" left="550.0" right="40.0" top="40.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
